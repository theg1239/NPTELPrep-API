# Base URL for the production API.
NPTEL_API_URL=https://api.nptelprep.in

# PostgreSQL connection string pointing to the production database.
DATABASE_URL=postgresql://user:password@host:5432/nptel

# Comma-separated Gemini API keys for the agent. The agent rotates keys on 429 responses.
GOOGLE_API_KEYS=key_one,key_two

# Optional: override the sleep duration (in ms) between full iterations. Defaults to 300000 (5 minutes).
# AGENT_LOOP_SLEEP_MS=300000

# Optional: directory where staged change proposals are stored. Defaults to agent/changes.
# AGENT_CHANGES_DIR=agent/changes
